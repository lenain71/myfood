{
  "Logging": {
    "File": {
      "IncludeScopes": true,
      "DateFormat": "yyyyMMdd",
      "CounterFormat": "000",
      "MaxFileSize": 10485760,
      "BasePath": "Logs",
      "LogLevel": {
        "myfoodapp.Core": "Trace",
        "Default": "Error"
      },
      "Files": [
        { "Path": "<date:yyyy>/<date:MM>/trace-<counter>.log" }
      ]
    }
  }
}